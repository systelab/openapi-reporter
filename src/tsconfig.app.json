{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "paths": {
      "environments/*": ["environments/*"],
      "@common/*": ["app/common/*"],
      "@components": ["app/components/index.ts"],
      "@features/*": ["app/features/*"],
      "@jama/*": ["app/jama/*"],
      "@model": ["app/model/index.ts"],
      "@services/*": ["app/service/index.ts"]
    },
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
